#ifndef ir_result_hpp
#define ir_result_hpp

#include <string>

class IrResult {
 private:
  std::string name_;
  double score_;

 public:
  IrResult(std::string name, double score):
    name_(name), score_(score) {}
  
  std::string name() {
    return name_;
  }
  
  double score() {
    return score_;
  }
};

#endif /* ir_result_hpp */
